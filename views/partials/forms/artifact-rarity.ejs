<% if ( tabTitle === 'New Artifact' ) { %>
    <% for ( let i =1; i <= 5; i++ ) { %>
        <option value="<%= i %> Star"><%= i %> Star</option>
    <% } %>
<% } else if ( tabTitle === 'Edit Artifact' ) { %>
    <% for ( let i =1; i <= 5; i++ ) { %>
        <option value="<%= i %> Star"
            <% if ( artifact.rarity ===  i + " Star" ) { %>
                selected
            <% } %>     
        ><%= i %> Star</option>
    <% } %>
<% } %>